  readonly attribute unsigned short <a href="https://www.w3.org/TR/2011/WD-html5-20110525/the-iframe-element.html#dom-texttrack-readystate" title="dom-TextTrack-readyState">readyState</a>;
           attribute <a href="https://www.w3.org/TR/2011/WD-html5-20110525/webappapis.html#function">Function</a> <a href="https://www.w3.org/TR/2011/WD-html5-20110525/the-iframe-element.html#handler-texttrack-onload" title="handler-TextTrack-onload">onload</a>;
           attribute <a href="https://www.w3.org/TR/2011/WD-html5-20110525/webappapis.html#function">Function</a> <a href="https://www.w3.org/TR/2011/WD-html5-20110525/the-iframe-element.html#handler-texttrack-onerror" title="handler-TextTrack-onerror">onerror</a>;

  const unsigned short <a href="https://www.w3.org/TR/2011/WD-html5-20110525/the-iframe-element.html#dom-texttrack-off" title="dom-TextTrack-OFF">OFF</a> = 0;
